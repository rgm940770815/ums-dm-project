    <select id="selectDistinctAllUser" resultMap="BaseResultMap" parameterType="java.util.Map" >
        select f.* FROM (
        SELECT s.*,ROW_NUMBER() OVER (PARTITION BY org_code ORDER BY id) AS group_idx FROM (
        SELECT
        org_code,
        dept_name,
        id       ,
        org_type ,
        type     ,
        sort_no
        FROM xtpt_bm_departement d INNER JOIN
        (SELECT
        b.org_code AS orgcode
        from
        ums_department a
        LEFT JOIN xtpt_bm_departement b
        on
        a.org_code = b.org_code
        where
        1=1
        AND b.org_code IS NOT NULL
        <if test="courtCode != null">
            AND a.court_code=#{courtCode}
        </if>
        <if test="courtLevel != null ">
            AND (SELECT court_gradation as level from ums_court_full where court_no = a.court_no) =#{courtLevel}
        </if>
        group by
        b.org_code) AS c
        ON d.org_code = c.orgcode
        ORDER BY id
        ) s
        ) f WHERE f.group_idx = 1
    </select>
